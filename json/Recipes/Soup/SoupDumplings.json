    "method": [
        {
            "text": "Fill a pot with salted water and boil",
            "post_notes": [],
            "pre_notes": [],
            "type": "METHOD_STEP"
        },
        {
            "text": "Put the butter, water and salt in a small saucepan and melt over medium heat",
            "post_notes": [],
            "pre_notes": [],
            "type": "METHOD_STEP"
        },
        {
            "text": "Add the \ufb02our and stir until the dough steams a bit and starts to smell nutty, but without coloring - about 4-5 minutes",
            "post_notes": [],
            "pre_notes": [],
            "type": "METHOD_STEP"
        },
        {
            "text": "Take off the heat and mix in the mustard",
            "post_notes": [],
            "pre_notes": [],
            "type": "METHOD_STEP"
        },
        {
            "text": "Gradually beat in the egg, then the herbs",
            "post_notes": [],
            "pre_notes": [],
            "type": "METHOD_STEP"
        },
        {
            "text": "Form the dumplings, and cook in the boiling water for about 5 minutes (open one to see if its done)",
