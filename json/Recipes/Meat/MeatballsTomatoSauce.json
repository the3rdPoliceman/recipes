    "method": [
        {
            "text": "Beat the cream and egg together, then add the breadcrumbs, salt, parsley and nutmeg, and leave to stand for 10 minutes",
            "post_notes": [],
            "pre_notes": [],
            "type": "METHOD_STEP"
        },
        {
            "text": "Mix the onion and grated parmesan into the meat, the stir in the cream mixture",
            "post_notes": [],
            "pre_notes": [],
            "type": "METHOD_STEP"
        },
        {
            "text": "Beat your meat well, and form into balls",
            "post_notes": [],
            "pre_notes": [],
            "type": "METHOD_STEP"
        },
        {
            "text": "Fry in olive oil until brown",
            "post_notes": [],
            "pre_notes": [],
            "type": "METHOD_STEP"
        },
        {
