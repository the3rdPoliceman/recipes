    "method": [
        {
            "text": "Finely chop the onion and fry slowly in the butter until well caramelized",
            "post_notes": [],
            "pre_notes": [],
            "type": "METHOD_STEP"
        },
        {
            "text": "Mix 150ml of the cream and bredcrumbs and set aside for at least 10 minutes",
            "post_notes": [],
            "pre_notes": [],
            "type": "METHOD_STEP"
        },
        {
            "text": "Mix the egg, salt, spices and onion into the cream. Then add this all to the meat",
            "post_notes": [],
            "pre_notes": [],
            "type": "METHOD_STEP"
        },
        {
            "text": "Beat the meat vigorously for a couple of minutes until it's really well mixed, then form into walnut sized meatballs (water on hands makes this easier)",
            "post_notes": [],
            "pre_notes": [],
            "type": "METHOD_STEP"
        },
        {
            "text": "Fry in the onion pan with a little oil if needed, adding a little butter towards the end",
            "post_notes": [],
            "pre_notes": [],
            "type": "METHOD_STEP"
        },
        {
            "text": "When the meatballs are done, take them out and add the rest of the cream to the pan to make a sauce",
            "post_notes": [],
            "pre_notes": [],
            "type": "METHOD_STEP"
        },
        {
            "text": "Serve with the berries, cucumber, and mashed potato",
            "post_notes": [],
            "pre_notes": [],
            "type": "METHOD_STEP"
        }
    ],
