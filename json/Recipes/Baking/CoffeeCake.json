  "method": [
    {
      "type": "METHOD_SUB_LIST",
      "text": "Cake",
      "method_steps": [
        {
          "post_notes": [],
          "pre_notes": [],
          "type": "METHOD_STEP",
          "text": "Roast the nuts, then chop the walnuts, and grind the hazelnuts into powder"
        },
        {
          "post_notes": [],
          "pre_notes": [],
          "type": "METHOD_STEP",
          "text": "Cream the butter and sugar, stir in the nuts, and gradually beat in the eggs"
        },
        {
          "post_notes": [],
          "pre_notes": [],
          "type": "METHOD_STEP",
          "text": "Combine the flour, baking powder and salt, and fold into the mix"
        },
        {
          "post_notes": [],
          "pre_notes": [],
          "type": "METHOD_STEP",
          "text": "Combine the coffee vanilla and oil, and fold into the mix"
        },
        {
          "post_notes": [],
          "pre_notes": [],
          "type": "METHOD_STEP",
          "text": "Bake at 160C until done"
        }
      ]
    },
    {
      "type": "METHOD_SUB_LIST",
      "text": "Icing",
      "method_steps": [
        {
          "post_notes": [],
          "pre_notes": [],
          "type": "METHOD_STEP",
          "text": "Dissolve the coffee powder in the vanilla"
        },
        {
          "post_notes": [],
          "pre_notes": [],
          "type": "METHOD_STEP",
          "text": "Beat the butter well until soft"
        },
        {
          "post_notes": [],
          "pre_notes": [],
          "type": "METHOD_STEP",
          "text": "Seve the icing sugar, and gradually beat into the butter"
        },
        {
          "post_notes": [],
          "pre_notes": [],
          "type": "METHOD_STEP",
          "text": "Finally, beat in the coffee/vanilla mix"
        }
      ]
    }
  ],
