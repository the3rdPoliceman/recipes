  "method": [
    {
      "post_notes": [],
      "pre_notes": [],
      "type": "METHOD_STEP",
      "text": "Beat the butter and sugar until pale and light - about 10 minutes"
    },
    {
      "post_notes": [],
      "pre_notes": [],
      "type": "METHOD_STEP",
      "text": "Beat the eggs in a small bowl, and gradually beat into the butter and sugar"
    },
    {
      "post_notes": [],
      "pre_notes": [],
      "type": "METHOD_STEP",
      "text": "Combine the flour, baking powder, ground pistachios, ground almonds, lemon zest and salt in a bowl, then fold into the butter mix, stirring as little as possible"
    },
    {
      "post_notes": [],
      "pre_notes": [],
      "type": "METHOD_STEP",
      "text": "Put the dough in a lined 18cm pan and bake until it starts coming away from the sides and a stick comes out clean. About 35 minutes"
    },
    {
      "post_notes": [],
      "pre_notes": [],
      "type": "METHOD_STEP",
      "text": "Meanwhile, heat the lemon juice and sugar until the sugar dissolves. Add the chopped pistachios, bring to a boil and take off the heat"
    },
    {
      "post_notes": [],
      "pre_notes": [],
      "type": "METHOD_STEP",
      "text": "Remove the cake from the tin, and take the baking paper off the bottom but leave it on the sides. Pour the hot pistachio syrup over the top and let everything cool down completely, then remove the baking paper from the sides"
    }
  ],
  "notes": [],
