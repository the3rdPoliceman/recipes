  "method": [
    {
      "post_notes": [],
      "pre_notes": [],
      "type": "METHOD_STEP",
      "text": "Heat the oven to 170C. Roast the almonds until they start to brown slightly, then mix with the sugar and butter and roast for another 10 minutes. Leave to cool, then chop roughly"
    },
    {
      "post_notes": [],
      "pre_notes": [],
      "type": "METHOD_STEP",
      "text": "Put the milk in a pan and heat until simmering"
    },
    {
      "post_notes": [],
      "pre_notes": [],
      "type": "METHOD_STEP",
      "text": "Whisk the egg yolks, sugar and salt together, then mix in the milk"
    },
    {
      "post_notes": [],
      "pre_notes": [],
      "type": "METHOD_STEP",
      "text": "Put back in the pan, and reheat until it forms a thin custard"
    },
    {
      "post_notes": [],
      "pre_notes": [],
      "type": "METHOD_STEP",
      "text": "Mix in the cream, vanilla, and almonds"
    },
    {
      "post_notes": [],
      "pre_notes": [],
      "type": "METHOD_STEP",
      "text": "Chill thoroughly and then churn"
    }
