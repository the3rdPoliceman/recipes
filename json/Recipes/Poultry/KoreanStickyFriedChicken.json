  "method": [
    {
      "post_notes": [],
      "pre_notes": [],
      "type": "METHOD_STEP",
      "text": "Blitz all the sauce ingredients in a food processor, then let bubble gently for a few minutes in a saucepan"
    },
    {
      "post_notes": [],
      "pre_notes": [],
      "type": "METHOD_STEP",
      "text": "For the chicken, mix with the ginger, then add the cornflour, salt and pepper"
    },
    {
      "post_notes": [],
      "pre_notes": [],
      "type": "METHOD_STEP",
      "text": "Deep fry in 180-190C oil, then stir fry quickly in the sauce"
    },
    {
      "post_notes": [],
      "pre_notes": [],
      "type": "METHOD_STEP",
      "text": "Sprinkle the spring onion and sesame seeds over when serving"
    }
